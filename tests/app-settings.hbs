version = 1

[app]
displayVersion = {{set_time}}
frameRate = {{set_value "app|frameRate"}}
consolePort = {{set_value "console|port"}}
assetServer = {{set_value "download|server"}}

[screen]
windowed = {{set_value "screen|windowed"}}
width = {{set_value "screen|width"}}
height = {{set_value "screen|height"}}

[server]
relay = {{set_value "server|relay"}}
port = {{set_value "server|port"}}
etcdIp = {{set_value "server|etcdIp"}}
etcdPort = {{set_value "server|etcdPort"}}
autoStart = {{set_value "server|autoStart"}}